{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "TanStack Start",
	"extends": "./base.json",
	"compilerOptions": {
		"jsx": "react-jsx",
		"moduleResolution": "Bundler",
		"module": "esnext",
		"target": "es2022",
		"skipLibCheck": true,
		"strictNullChecks": true,
		"verbatimModuleSyntax": false
